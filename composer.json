{
  "name": "slick/configuration",
  "description": "Slick/Configuration is a simple package that deals with configuration files.",
  "keywords": [
    "slick",
    "configuration",
    "INI",
    "arrays"
  ],
  "homepage": "https://github.com/slickframework/configuration",
  "license": "MIT",
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=5.4.0",
    "slick/common": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1",
    "behat/behat": "~3.0.4"
  },
  "autoload": {
    "psr-4": {
      "Slick\\Configuration\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slick\\Tests\\Configuration\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev",
      "dev-develop": "1.2-dev"
    }
  }
}